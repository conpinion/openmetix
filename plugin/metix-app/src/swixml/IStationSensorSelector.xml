<panel layout="GridBagLayout">

	<gridbagconstraints id="std" 
		insets="2,2,2,2" ipadx="0" ipady="0" 
		gridx="0" gridy="0" gridheight="1" gridwidth="1"
		weightx="0.0" weighty="0.0" 
		anchor="GridBagConstraints.WEST"
		fill="GridBagConstraints.NONE"/>

		<gridbagconstraints use="std" gridx="0" gridy="0" weightx="1.0" fill="GridBagConstraints.HORIZONTAL" insets="0,0,0,0"/>

		<ifieldlabel text="metix.station">
			<gridbagconstraints use="std" gridx="0" gridy="0" insets="0,0,0,2"/>
		</ifieldlabel>

		<combobox id="stations">
			<gridbagconstraints use="std" gridx="1" gridy="0" weightx="1.0" fill="GridBagConstraints.HORIZONTAL" insets="0,2,0,0"/>
		</combobox>

		<ifieldlabel text="metix.parameter">
			<gridbagconstraints use="std" gridx="0" gridy="1" insets="2,0,0,2"/>
		</ifieldlabel>

		<combobox id="parameters">
			<gridbagconstraints use="std" gridx="1" gridy="1" weightx="1.0" fill="GridBagConstraints.HORIZONTAL" insets="2,2,0,0"/>
		</combobox>
</panel>